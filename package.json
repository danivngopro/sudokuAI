  "bugs": {
    "url": "https://gitlab.com/yesodot/selenium/apollo/omek/position-crud/issues"
  },
  "homepage": "https://gitlab.com/yesodot/selenium/apollo/omek/position-crud#readme",
  "dependencies": {
    "@bit/omek.utils.error-handler": "^0.0.1",
    "@bit/omek.utils.errors": "^0.0.1",
    "@bit/omek.utils.logger": "^1.0.3",
    "@bit/omek.utils.wrapper": "^1.0.2",
    "body-parser": "^1.19.0",
    "env-var": "^7.1.1",
    "express": "^4.17.1",
    "joi": "^17.4.0",
    "mongoose": "^6.0.12",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
