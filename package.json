{
  "name": "position-crud",
  "version": "1.0.0",
  "description": "Service Template",
  "main": "",
  "scripts": {
    "install:prod": "npm install --production",
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "ts-node ./src/index.ts",
    "lint:fix": "eslint -c .eslintrc.json --no-error-on-unmatched-pattern ./src/**/* --fix",
    "lint": "eslint -c .eslintrc.json --no-error-on-unmatched-pattern ./src/**/*",
    "test": "tsc && jest --maxWorkers=1 --testPathPattern .spec.ts$",
    "test:unit": "tsc && jest --testPathPattern *.manager.spec.ts",
    "test:integration": "tsc && jest --maxWorkers=1 --testPathPattern .router.spec.ts$",
    "coverage": "tsc && jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/yesodot/selenium/apollo/omek/position-crud.git"
